  "bugs": {
    "url": "https://github.com/raarnout/web-scraper/issues"
  },
  "homepage": "https://github.com/raarnout/web-scraper#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
