{
  "name": "web-scraper",
  "version": "1.0.0",
  "description": "node.js web scraper tool",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --config nodemon.json src/index.js",
    "full": "nodemon -x 'clear;printf \"\\e[3J\";node' --ignore ./dist/ src/full_credits.js",
    "featured": "nodemon -x 'clear;printf \"\\e[3J\";node' --ignore ./dist/ src/featured_credits.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raarnout/web-scraper.git"
  },
  "keywords": [
    "scraper",
    "node.js"
  ],
